{
    "version": "1.0.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch",
        "cwd": "${workspaceFolder}",
        "runtimeExecutable": null,
        "args": [],
        "stopOnEntry": false,
        "runtimeArgs": [ "--nolazy" ],
        "preLaunchTask": null,
        "env": {
          "NODE_ENV": "development",
          "LOG_LEVEL": "debug"
        },
        "console": "internalConsole",
        "program": "${workspaceFolder}/index.js",
        "outFiles": []
      },
      {
        "type": "node",
        "request": "attach",
        "name": "Attach",
        "port": 9229,
        "address": "localhost",
        "restart": false,
        "sourceMaps": false,
        "outFiles": [],
        "skipFiles": [
          "<node_internals>/**/*.js"
        ]
      }
    ]
  }